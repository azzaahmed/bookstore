<p id="notice"><%= notice %></p>

<h1>Listingitems</h1>

<table>
  <thead>
    <tr>
      <th> id</th>
      <th> name</th>
      <th> price when purchased</th>
      
      <th>current price</th>
      <th> quantity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @items.each do |book| %>
      <tr>
       
        <td><%= book.item.id %></td>
        <td><%= book.item.name %></td>

         <td><%= book.price %></td>
      <td></td>
           <td><%= book.item.price %></td>
            <td> <%= book.quantity %> </td>

       
      </tr>
    <% end %>
  </tbody>
</table>

